<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="classSQL">
<insert id="classAdd" parameterType="papa.class_.model.classDTO">
insert into class_info(name,subject,class_time,institut,class_date,write_date,explanation,location) 
values(#{name},#{subject},#{class_time},#{institut},#{class_date},now(),#{explanation},#{location})
</insert>
</mapper>